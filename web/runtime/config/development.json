{
  "port": 8080,
  "workers": 1,
  "stickyCluster": false,
  "startServerTimeout": 3000,
  "reloadSignal": "SIGUSR2",
  "processKillTimeout": 10000,
  "jsonpCallbackField": "callback",
  "jsonContentType": "application/json",
  "jsonpContentType": "application/javascript",
  "errnoField": "errno",
  "errmsgField": "errmsg",
  "defaultErrno": 1000,
  "validateDefaultErrno": 1001,
  "view": {
    "type": "nunjucks",
    "nunjucks": {
      "viewPath": "/Users/welefen/Develop/git/grid-layout/web/view",
      "sep": "_",
      "extname": ".html"
    }
  },
  "logger": {
    "type": "console",
    "console": {},
    "file": {
      "backups": 10,
      "absolute": true,
      "maxLogSize": 51200,
      "filename": "/Users/welefen/Develop/git/grid-layout/web/logs/app.log"
    },
    "dateFile": {
      "level": "ALL",
      "absolute": true,
      "pattern": "-yyyy-MM-dd",
      "alwaysIncludePattern": true,
      "filename": "/Users/welefen/Develop/git/grid-layout/web/logs/app.log"
    }
  }
}